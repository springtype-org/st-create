{
    "name": "st-create",
    "version": "2.0.0-beta.8",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "springtype",
        "scaffold",
        "generator",
        "code generator",
        "create"
    ],
    "description": "Generates SpringType projects and components for you.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/springtype-org/springtype.git"
    },
    "scripts": {
        "build": "st-run st-rm-rf dist ++ system:tsc ++ st-cp package.json node_modules template README.md dist",
        "test": "ts-node test.ts",
        "start": "node ./dist/cli.js"
    },
    "engines": {
        "node": ">=10"
    },
    "bundledDependencies": [
        "envinfo",
        "chalk",
        "st-rm-rf",
        "inquirer",
        "commander",
        "cross-spawn",
        "validate-npm-package-name"
    ],
    "bin": {
        "st-create": "./cli.js"
    },
    "main": "cli.js",
    "author": "Aron Homberg <info@aron-homberg.de>, Michael Mannseicher <m.mansi1@gmx.net>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/springtype-org/springtype/issues"
    },
    "homepage": "https://www.springtype.org",
    "dependencies": {
        "chalk": "^4.0.0",
        "commander": "^5.1.0",
        "cross-spawn": "^7.0.2",
        "envinfo": "^7.5.0",
        "inquirer": "^7.1.0",
        "st-rm-rf": "^2.0.0-beta.1",
        "validate-npm-package-name": "^3.0.0"
    },
    "devDependencies": {
        "@types/node": "^13.13.2",
        "st-run": "^2.0.0-beta.4",
        "ts-node": "^8.9.1",
        "typescript": "^3.8.3"
    }
}
